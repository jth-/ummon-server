{
  "name":"Sample",
  "cwd":"/var/www/website/", 
  "log": "",
  "notify": "",
  
  "sequences": { 
    "update-encounters": {
      "tasks": ["task1", "task2", "task3"],
      "log": "",
      "cwd": ""
    }
  },

  
  "tasks": {
    "task1": {
      "cwd": "/var/www/website2/",
      "command": "./symfony w2h:update-apis",
      "arguments": ["--verbose", "--env=staging"],
      "trigger": {
        "time": "*/10 * * * *"
      }
    },
    "task2": {
      "command": "./symfony w2h:process-data",
      "arguments": ["--verbose", "--env=staging"],
      "trigger": {
        "after": "task1",
        "before": "task3"
      }
    },
    "task3": {
      "command": "./symfony w2h:process-data",
      "arguments": ["--verbose", "--env=staging"]
    },
    "task4": {
      "command": "./symfony w2h:process-data",
      "arguments": ["--verbose", "--env=staging"]
    }
  }
}